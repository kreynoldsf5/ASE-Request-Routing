              "shareNodes": true,
              "serverAddresses": [
                "10.1.10.5"
              ]
            }]
        },
        "logo-pool": {
            "class": "Pool",
            "monitors": [
              "http"
            ],
            "members": [{
              "servicePort": 8282,
              "shareNodes": true,
              "serverAddresses": [
                "10.1.10.5"
              ]
            }]
        },
        "api-pool": {
            "class": "Pool",
            "monitors": [
              "http"
            ],
            "members": [{
              "servicePort": 8383,
              "shareNodes": true,
              "serverAddresses": [
                "10.1.10.5"
              ]
